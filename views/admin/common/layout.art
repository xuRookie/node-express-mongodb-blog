<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Blog - Content Manager</title>
    <link rel="icon" href="{{prodPath}}/images/favicon.ico" />
    <link rel="stylesheet" href="{{prodPath}}/vendor/simple-line-icons/css/simple-line-icons.css">
    <link rel="stylesheet" href="{{prodPath}}/vendor/font-awesome/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="{{prodPath}}/css/bootstrap.min.css">
    <link rel="stylesheet" href="{{prodPath}}/css/styles.css">
    <link rel="stylesheet" href="{{prodPath}}/css/common.css">
    {{block 'link'}}{{/block}}
</head>
<body class="sidebar-fixed header-fixed">
    <div class="page-wrapper">
        {{include './header.art'}}

        <div class="main-container">
            {{include './aside.art'}}

            <div class="content">
                {{block 'main'}}{{/block}}
            </div>
        </div>
    </div>
    <script src="{{prodPath}}/js/jquery.slim.min.js"></script>
    <script src="{{prodPath}}/js/popper.min.js"></script>
    <script src="{{prodPath}}/js/bootstrap.min.js"></script>
    <script src="{{prodPath}}/js/jquery.validate.min.js"></script>
    <script src="{{prodPath}}/vendor/chart.js/chart.min.js"></script>
    <script src="{{prodPath}}/js/carbon.js"></script>
    <script src="{{prodPath}}/js/demo.js"></script>
    <script src="{{prodPath}}/js/utils.js"></script>
    <script src="{{prodPath}}/js/wangEditor.min.js"></script>
    {{block 'script'}}{{/block}}
</body>
</html>
